<h1>ログイン</h1>

<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

<%= f.label :email,"登録メールアドレス" %>

<%= f.email_field :email %>

<br>

<%= f.label :password, "パスワード　　　　" %>

<%= f.password_field :password %>

<%= f.submit 'ログイン' %>

<% end %>

<br>
<p>登録はこちらから↓</p>

<%= link_to '新規登録', signup_path %>